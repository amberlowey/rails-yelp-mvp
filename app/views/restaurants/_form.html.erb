<%= form_for(@restaurant) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :phone_number %>
  <%= f.text_field :phone_number %>
  <%= f.label :category %>
  <%= f.select :category, ['Chinese', 'Italian', 'Japanese', 'French', 'Belgian'], include_blank: true %>
  <br>
  <%= f.submit "Add Restaurant", class: "btn btn-primary"%>
<% end %>
